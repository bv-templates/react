{
  "name": "react",
  "version": "1.0.0",
  "description": "{{ description }}",
  "main": "src/index.js",
  "config": {
    "port": "8080"
  },
  "scripts": {
    "prestart": "npm run clean & npm run clone",
    "start": "npm run build:dll && cross-env NODE_ENV=development webpack-dev-server",
    "prebuild": "npm run clean & npm run clone",
    "build": "npm run build:dll && cross-env NODE_ENV=production webpack",
    "postbuild": "npm run cdn",
    "build:dll": "cross-env NODE_ENV=production webpack --config webpack.dll.config.js",
    "clone": "bv-git clone",
    "cdn": "bv-cdn upload",
    "clean": "rimraf dist",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      {{#eslint}}
      "eslint --fix",
      {{/eslint}}
      {{#prettier}}
      "prettier --write",
      {{/prettier}}
      "git add"
    ],
    "*.{css,scss,less}": [
      {{#stylelint}}
      "stylelint --fix",
      {{/stylelint}}
      "git add"
    ]
  },
  "author": "{{ author }}",
  "license": "ISC",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-minify": "^0.5.0",
    "bv-cdn-cli": "^0.0.2",
    "bv-git-cli": "0.0.7",
    "core-js": "^3.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "download-git-repo": "^1.1.0",
    {{#eslint}}
    "babel-eslint": "^10.0.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-react": "^7.13.0",
    {{/eslint}}
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-include-assets-plugin": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "image-webpack-loader": "^4.6.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "ora": "^3.4.0",
    "path": "^0.12.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    {{#prettier}}
    "prettier": "^1.17.1",
    {{/prettier}}
    "react-hot-loader": "^4.8.4",
    "react-loadable": "^5.5.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    {{#stylelint}}
    "stylelint": "^10.0.1",
    "stylelint-config-idiomatic-order": "^7.0.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.6.1",
    {{/stylelint}}
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  }
}
